{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq",
      "Serilog.Exceptions",
      "Serilog.Enrichers.Environment"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "WithExceptionDetails",
      "FromLogContext",
      "WithEnvironmentName",
      "WithEnvironmentUserName",
      "WithMachineName",
      "WithThreadId",
      "WithThreadName"
    ],
    "Properties": {
      "ApplicationName": "Store"
    }
  }
}